Policy: /mnt/livecd/usr/sbin/pdnsd, Emulation: linux64
	linux64-accept: permit
	linux64-arch_prctl: permit
	linux64-bind: sockaddr match "inet-*" then permit
	linux64-sendmsg: sockaddr match "inet-*" then permit
	linux64-connect: sockaddr match "inet-*:53" then permit
	linux64-brk: permit
	linux64-clone: permit
	linux64-close: permit
	linux64-exit: permit
	linux64-fsread: filename match "/proc*" then deny
	linux64-fsread: filename match "/sys*" then deny
	linux64-fsread: filename eq "/dev/urandom" then permit
	linux64-fsread: filename eq "/etc/group" then permit
	linux64-fsread: filename eq "/etc/hosts" then permit
	linux64-fsread: filename eq "/etc/ld.so.cache" then permit
	linux64-fsread: filename eq "/etc/ld.so.preload" then permit
	linux64-fsread: filename eq "/etc/localtime" then permit
	linux64-fsread: filename eq "/etc/nsswitch.conf" then permit
	linux64-fsread: filename eq "/etc/passwd" then permit
	linux64-fsread: filename eq "/lib64/libc.so.6" then permit
	linux64-fsread: filename eq "/lib64/libnsl.so.1" then permit
	linux64-fsread: filename eq "/lib64/libnss_compat.so.2" then permit
	linux64-fsread: filename eq "/lib64/libnss_files.so.2" then permit
	linux64-fsread: filename eq "/lib64/libnss_nis.so.2" then permit
	linux64-fsread: filename eq "/lib64/libpthread.so.0" then permit
	linux64-fsread: filename eq "/proc/sys/kernel/ngroups_max" then permit
	linux64-fsread: filename eq "/usr/local/etc/pdnsd/pdnsd.conf" then permit
	linux64-fsread: filename match "/var/cache/pdnsd*" then permit
	linux64-fstat: permit
	linux64-fswrite: filename match "/var/cache/pdnsd*" then permit
	linux64-getrlimit: permit
	linux64-getuid: permit
	linux64-listen: permit
	linux64-lseek: permit
	linux64-madvise: permit
	linux64-mmap: prot eq "PROT_NONE" then permit
	linux64-mmap: prot eq "PROT_READ" then permit
	linux64-mmap: prot eq "PROT_READ|PROT_EXEC" then permit
	linux64-mmap: prot eq "PROT_READ|PROT_WRITE" then permit
	linux64-mprotect: prot eq "PROT_NONE" then permit
	linux64-mprotect: prot eq "PROT_READ" then permit
	linux64-mprotect: prot eq "PROT_READ|PROT_WRITE" then permit
	linux64-munmap: permit
	linux64-read: permit
	linux64-recvmsg: permit
	linux64-rt_sigaction: permit
	linux64-rt_sigprocmask: permit
	linux64-rt_sigtimedwait: permit
	linux64-set_robust_list: permit
	linux64-set_tid_address: permit
	linux64-setgid: permit
	linux64-setgroups: permit
	linux64-setsockopt: permit
	linux64-setuid: permit
	linux64-socket: permit
	linux64-umask: permit
	linux64-write: permit
	linux64-exit: permit
	linux64-exit_group: permit
	linux64-ioctl: permit
	linux64-fcntl: permit
	linux64-getsockopt: permit
	linux64-poll: permit
	linux64-writev: permit
	linux64-exit: permit
	linux64-exit_group: permit
	linux64-futex: permit
